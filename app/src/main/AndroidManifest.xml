<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="SortViz"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MergeSort"
        tools:targetApi="31">


        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="SortViz"
            android:theme="@style/Theme.MergeSort">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity android:name=".GraphActivity" />
        <activity android:name=".SortActivity" />
        <activity android:name=".MergeSortActivity" />
        <activity android:name=".BubbleSortActivity" />
        <activity android:name=".QuickSortActivity" />
        <activity android:name=".InsertionSortActivity" />
        <activity android:name=".SelectionSortActivity" />
    </application>

</manifest>